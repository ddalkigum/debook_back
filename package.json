{
  "name": "goback",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc --build",
    "start": "npm run build && export NODE_ENV=production && node ./dist/server.js",
    "start:dev": "export NODE_ENV=development && ts-node ./src/server.ts",
    "start:local": "export NODE_ENV=local && nodemon ./src/server.ts",
    "start:test": "export NODE_ENV=test && nodemon ./src/server.ts",
    "test:unit": "export NODE_ENV=test && jest --config jest.unit.config.json --detectOpenHandles",
    "test:integration": "export NODE_ENV=test && jest --config jest.integration.config.json --detectOpenHandles",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "redis": "^4.0.6",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.3",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
