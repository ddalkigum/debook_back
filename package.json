{
  "name": "debook_back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc --build",
    "start": "export NODE_ENV=production && pm2 start ecosystem.config.js --env production",
    "start:dev": "export NODE_ENV=development && pm2 start ecosystem.config.js --env development",
    "start:local": "export NODE_ENV=local && nodemon ./src/server.ts",
    "start:local:pm2": "export NODE_ENV=local && pm2 start ecosystem.config.js --name debook_local --env local",
    "start:local:https": "export NODE_ENV=local && export SERVER_TYPE=https && nodemon ./src/server.ts",
    "test:unit": "export NODE_ENV=test && jest --config jest.unit.config.json --detectOpenHandles",
    "test:integration": "export NODE_ENV=test && jest --config jest.integration.config.json --detectOpenHandles",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "migration": "export NODE_ENV=production && npx typeorm-ts-node-commonjs migration:run -d src/infrastructure/database/maria/ormConfig.ts",
    "migration:local": "export NODE_ENV=local && npx typeorm-ts-node-commonjs migration:run -d src/infrastructure/database/maria/ormConfig.ts",
    "migration:revert": "export NODE_ENV=production && npx typeorm-ts-node-commonjs migration:revert -d src/infrastructure/database/maria/ormConfig.ts",
    "migration:revert:local": "export NODE_ENV=production && npx typeorm-ts-node-commonjs migration:revert -d src/infrastructure/database/maria/ormConfig.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.75.0",
    "@slack/web-api": "^6.7.2",
    "aws-sdk": "^2.1148.0",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "googleapis": "^104.0.0",
    "helmet": "^5.0.2",
    "inversify": "^6.0.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "sharp": "^0.30.7",
    "ts-node": "^10.8.0",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.12",
    "@types/sharp": "^0.30.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
